{
  "libraryName": "amazonka-dynamodb",
  "extraDependencies": [
    "aeson",
    "amazonka-dynamodb-attributevalue",
    "containers"
  ],
  "typeModules": [
    "Amazonka.DynamoDB.Types.AttributeValue",
    "Amazonka.DynamoDB.Types.WriteRequest"
  ],
  "typeOverrides": {
    "AttributeValue": {
      "replacedBy": {
        "name": "AttributeValue",
        "underive": []
      }
    },
    "BatchGetItemOutput": {
      "requiredFields": ["Responses", "UnprocessedKeys"]
    },
    "BatchWriteItemOutput": {
      "requiredFields": ["UnprocessedItems"]
    },
    "DeleteRequest": {
      "replacedBy": {
        "name": "",
        "underive": []
      }
    },
    "PutRequest": {
      "replacedBy": {
        "name": "",
        "underive": []
      }
    },
    "QueryOutput": {
      "requiredFields": ["Items"]
    },
    "WriteRequest": {
      "replacedBy": {
        "name": "WriteRequest",
        "underive": []
      }
    }
  }
}
